<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="600.0" maxWidth="800.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.Player.Controller.Menu">
   <children>
      <ImageView pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <image>
            <Image url="@../../resources/ManagementGUI.png" />
         </image>
      </ImageView>
      <ImageView fx:id="btnVolume" fitHeight="25.0" fitWidth="50.0" layoutX="761.0" layoutY="536.0" onMouseClicked="#doSwapMusic" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="39.0" AnchorPane.rightAnchor="14.0">
         <image>
            <Image url="@../resources/sound_on.png" />
         </image>
      </ImageView>
    <Button layoutX="300.0" layoutY="251.0" mnemonicParsing="false" onAction="#openGame" onMouseEntered="#MouseEntered" prefHeight="26.0" prefWidth="194.0" styleClass="column-drag-header" text="Start New Game" />
      <Button layoutX="300.0" layoutY="300.0" mnemonicParsing="false" onAction="#openStatistics" onMouseEntered="#MouseEntered" prefHeight="26.0" prefWidth="194.0" styleClass="column-drag-header" text="View Statistics" />
      <Button layoutX="300.0" layoutY="350.0" mnemonicParsing="false" onAction="#doLogout" onMouseEntered="#MouseEntered" prefHeight="26.0" prefWidth="194.0" styleClass="column-drag-header" text="Logout" />
      <Label fx:id="lblUserLogged" layoutX="14.0" layoutY="14.0" prefHeight="18.0" prefWidth="117.0" styleClass="lblUser" text="lbl" />
      <Button layoutX="486.0" layoutY="488.0" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="140.0" text="Back" />
   </children>
</AnchorPane>
